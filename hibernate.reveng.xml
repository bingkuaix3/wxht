<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="admins" catalog="wxdb" class="Admin">
		<primary-key>
			<generator class="uuid.hex"></generator>
		</primary-key>
	</table>
	<table name="pages" catalog="wxdb" class="Page">
		<primary-key>
			<generator class="uuid.hex"></generator>
		</primary-key>
	</table>
	<table name="pagetypes" catalog="wxdb" class="PageType">
		<primary-key>
			<generator class="uuid.hex"></generator>
		</primary-key>
	</table>
	<table name="resources" catalog="wxdb" class="Resource">
		<primary-key>
			<generator class="uuid.hex"></generator>
		</primary-key>
	</table>
	<table name="resourcetypes" catalog="wxdb" class="ResourceType">
		<primary-key>
			<generator class="uuid.hex"></generator>
		</primary-key>
	</table>
<table name="users" catalog="wxdb" class="User">
	<primary-key>
		<generator class="uuid.hex"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>